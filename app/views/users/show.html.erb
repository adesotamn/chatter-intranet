<div id="feed" class="span-18 last">
	<br /><br />
	<div class="span-2" style="margin-bottom: 20px;">
		<%= image_cheater(@profile['lastName']) %>
	</div>
	<div class="span-6" style="margin-bottom: 20px;">
	  <h3><%= sanitize @profile['name'] %></h3>
	</div>
	
	<h3 class="span-10 last">
	  Telephone: <%= sanitize	@profile['phoneNumbers'][0]['number'] %>
	</h3>	
	<div class="span-18 last">
	  <h4>Current Status</h4>
	  <p class="explanation-text"><%= sanitize @profile['currentStatus']['body']['text'] %></p>
	</div>
	<div class="span-18 last">
	  <h4> About Me</h4>
	  <p class="explanation-text"><%= sanitize	@profile['aboutMe'] %></p>
    </div>
	
</div>